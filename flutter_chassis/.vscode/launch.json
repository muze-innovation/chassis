{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "flutter_chassis",
      "request": "launch",
      "type": "dart",

      // The entry script to execute when running the app.
		  // Set to a "web" in a Dart web app to run in web move.
		  // Set to "test" in an app with tests to run all tests.
      "program": "example/lib/main.dart",

      // Arguments to be passed to the Dart or Flutter app.
      "args": [
        "--no-sound-null-safety"
      ],
    },
    {
      "name": "flutter_chassis (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "flutter_chassis (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}